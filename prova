// Easy af, this program takes two numbers from the user and performs simple operations on them

// CODE BEGINS BELOW

// SPDX-License-Identifier: MIT
pragma solidity >0.6.9 <=0.8.9;

contract calc{
    function addTwoNumbers(uint num1, uint num2) public returns(uint) { return(num1+num2);}
    function subTwoNumbers(uint num1, uint num2) public returns(uint) {return(num1-num2);}
    function mulTwoNumbers(uint num1, uint num2) public returns(uint) {return(num1*num2);}
    function divTwoNumbers(uint num1, uint num2) public returns(uint) {return(num1/num2);}
    function modTwoNumbers(uint num1, uint num2) public returns(uint) {return(num1%num2);}
}

// END OF CODE
